{
  "name": "age-finder",
  "version": "0.1.0",
  "dependencies": {
    "moment": "^2.29.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "cypress run --reporter nyan",
    "test:ci": "start-server-and-test http://localhost:3000",
    "test:prod": "cypress run --config baseUrl=https://age-finder.vercel.app --reporter nyan",
    "cy:open": "cypress open",
    "cypress": "start-server-and-test http://localhost:3000 cy:open"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:cypress/recommended"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "cypress": "^7.5.0",
    "eslint-plugin-cypress": "^2.11.3",
    "start-server-and-test": "^1.12.5"
  }
}
